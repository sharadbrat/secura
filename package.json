{
  "name": "secura",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "start": "npm run serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --no-fix",
    "size": "npm run build && size-limit",
    "test": "npm run lint && vue-cli-service test:unit -- test --require test/setup.ts"
  },
  "dependencies": {
    "@sentry/browser": "5.15.4",
    "@sharadbrat/randomizer": "0.0.2",
    "axios": "0.21.1",
    "clipboard": "2.0.6",
    "core-js": "2.6.5",
    "crypto-js": "4.0.0",
    "es6-symbol": "3.1.3",
    "idb": "5.0.5",
    "inversify": "5.0.1",
    "inversify-inject-decorators": "3.1.0",
    "nanoid": "2.1.11",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.3",
    "vue": "2.6.10",
    "vue-class-component": "7.1.0",
    "vue-property-decorator": "8.3.0",
    "vue-router": "3.0.3",
    "vuedraggable": "2.24.0",
    "vuex": "3.5.1",
    "vuex-class": "0.3.2",
    "vuex-module-decorators": "1.0.1"
  },
  "devDependencies": {
    "@size-limit/preset-app": "2.2.4",
    "@types/chai": "4.2.11",
    "@types/mocha": "7.0.2",
    "@types/sinon": "9.0.0",
    "@types/sinon-chai": "3.2.4",
    "@vue/cli-plugin-babel": "3.11.0",
    "@vue/cli-plugin-eslint": "4.2.3",
    "@vue/cli-plugin-typescript": "4.0.5",
    "@vue/cli-plugin-unit-mocha": "4.3.1",
    "@vue/cli-service": "4.2.3",
    "@vue/eslint-config-airbnb": "4.0.0",
    "@vue/eslint-config-typescript": "4.0.0",
    "@vue/test-utils": "1.0.0-beta.33",
    "babel-eslint": "10.0.1",
    "chai": "4.2.0",
    "eslint": "5.16.0",
    "eslint-plugin-unused-imports": "0.1.2",
    "eslint-plugin-vue": "5.0.0",
    "git-describe": "4.0.4",
    "husky": "4.3.0",
    "mocha": "7.1.1",
    "node-sass": "4.14.1",
    "sass-loader": "8.0.0",
    "sinon": "9.0.2",
    "typescript": "3.5.3",
    "vue-template-compiler": "2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run size && npm run test"
    }
  }
}
